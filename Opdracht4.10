            const int zes = 6;
            const int zeven = 7;
            const int acht = 8;
            const int negen = 9;
            const int tien = 10;
            const int elf = 11;
            const int twaalf = 12;



            int getal;

            Console.WriteLine("Vul nummer maand in: ");
            getal = Convert.ToInt16(Console.ReadLine());

            if (getal == een)
                Console.WriteLine("Januari");

            else if (getal == twee)
